  "main": "index.js",
  "author": "Andre Pato",
  "repository": "https://github.com/anpato/exspresso",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "exspresso-cli": "./dist/index.js"
  },
  "publishConfig": {
