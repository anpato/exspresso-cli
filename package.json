{
  "name": "exspresso-cli",
  "version": "1.0.2",
  "main": "index.js",
  "author": "Andre Pato",
  "repository": "https://github.com/anpato/exspresso",
  "license": "MIT",
  "preferGlobal": true,
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "node --exec babel-node .",
    "clean": "rm -rf dist ",
    "build-babel": "babel lib  --out-dir dist ",
    "build": "yarn clean && yarn build-babel",
    "start": "babel-node ./lib/index.js",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "express",
    "mongodb",
    "postgres",
    "sequelize",
    "postgresql",
    "mongoose",
    "template",
    "sequelize-cli",
    "es5",
    "es6",
    "babel"
  ],
  "dependencies": {
    "babel-preset-es2015": "6",
    "commander": "^4.0.1",
    "fs": "^0.0.1-security",
    "inquirer": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.1"
  }
}
